{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error",
		"no-tabs": ["error", { "allowIndentationTabs": true }]
	}
}
